								android:layout_height="match_parent"
								tools:context="com.mttnow.android.saudia.checkin.CheckInBookingsFragment">

<include
	android:id="@+id/toolbar"
	layout="@layout/toolbar" />

<ListView
	android:id="@+id/listView"
