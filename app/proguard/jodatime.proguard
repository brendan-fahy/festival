# Keep these classes to prevent runtime error when calculating Periods
-keep class org.joda.convert.FromString.** { *; }
-keep class org.joda.convert.ToString.** { *; }

-keepclassmembers public class org.joda.time.DateTime { *; }
-keepclassmembers public class org.joda.time.DateTimeUtils { *; }
-keepclassmembers public class org.joda.time.**.** {
	*;
}

-dontwarn org.joda.convert.ToString
-dontwarn org.joda.convert.FromString

-dontwarn org.w3c.dom.bootstrap.DOMImplementationRegistry